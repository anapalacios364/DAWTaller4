<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
  <style>
    .foto-container {
      margin: 20px;
      padding: 20px;
      border: 1px solid #ccc;
    }
    .etiquetas {
      margin-top: 20px;
      padding: 10px;
      border: 1px solid #000;
      background-color: #f0f0f0;
    }
  </style>
</head>
<body>

  <div class="container text-center">
    <div class="row">
      <div class="col">
  <h2>Taller 4</h2>
  </div>
    </div>
    <div class="row">
      <div class="col-3">
      </div>
      <div class="col">
        <% arrFotos.forEach(foto => { %>
          <div class="foto-container">
            <h2><%= foto.titulo %></h2>
            <p><%= foto.descripcion %></p>
            <p>Calificaci√≥n: <%= foto.calificacion %></p>
            <img src="<%= foto.ruta %>" alt="<%= foto.titulo %>">
            <div class="etiquetas">
              <h3>Etiquetas</h3>
              <ul>
                <% foto.etiqueta.forEach(etiqueta => { %>
                  <li><%= etiqueta.texto %></li>
                <% }); %>
              </ul>
            </div>
          </div>
        <% }); %>
      </div>
    </div>
  </div>


  
</body>
</html>
